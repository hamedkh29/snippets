					<Default>Title</Default>
				</Literal>
			</Declarations>
			<Code Language="csharp">
			<![CDATA[
		
			[DisplayName("$displayName$")]
			[ForeignKey("$propType$Id")]
			//Priority($priority$)
			//ColVisible(true)
			//ColField($propName$)
			//Visible(true)
			//UiType(dropdown[Service=api/$propType$s/FindAll,Name=$propName$,Value=id])
			//ContainerClass(col-md-$containerClass$)
			public virtual $propType$ $propType$ {get;protected set;}
				[Required(ErrorMessage = "انتخاب $displayName$ الزامی است")]
				[DisplayName("$displayName$")]
				//Priority($priority$)
				public long $propType$Id {get;protected set;}
			$end$]]>
			</Code>
		</Snippet>
	</CodeSnippet>
